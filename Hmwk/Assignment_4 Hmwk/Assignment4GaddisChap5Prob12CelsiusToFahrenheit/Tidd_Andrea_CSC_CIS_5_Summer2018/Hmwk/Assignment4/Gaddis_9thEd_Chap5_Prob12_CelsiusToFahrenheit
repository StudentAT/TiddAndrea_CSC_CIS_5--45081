 * Purpose: Make a table of the conversion of Celsius to Fahrenheit from 0 to 20.
 */

#include <iostream>

using namespace std;


int main(int argc, char** argv) 
{
    
    float Fahrenheit = 0.0f;
    
    double Celsius;
    
    
    cout << "Celsius\t\tFahrenheit \n";
    cout << "-------------------------- \n";
    
    for (int i = 0; i <= 20; i++)
    {
            Fahrenheit = (9 * i)/5 + 32;
        
        cout << i << "\t\t" << Fahrenheit << "\n";
    }
 


    return 0;
}

