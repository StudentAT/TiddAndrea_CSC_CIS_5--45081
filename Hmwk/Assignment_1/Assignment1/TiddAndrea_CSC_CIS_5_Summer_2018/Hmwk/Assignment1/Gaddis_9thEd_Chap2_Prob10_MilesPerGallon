 * Purpose: Calculate Miles per gallon
 */

#include <iostream>

using namespace std;

int main(int argc, char** argv) 
{
    int Miles, Gallons, MPG;
    
    Miles = 375;
    Gallons = 15;
    MPG = Miles / Gallons;
    
    // Calculate the MPG  
    
    cout << "Miles per Gallon = ";
    cout << Miles << " / " <<  Gallons << " = " << MPG;

    return 0;
}

